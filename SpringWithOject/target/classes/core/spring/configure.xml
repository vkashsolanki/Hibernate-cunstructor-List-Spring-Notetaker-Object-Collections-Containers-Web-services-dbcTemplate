<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        
        
        
        
        
        
		<bean id="address1" class="core.spring.Address">
		<property name="addressline1" value="Singh-Lane Street"></property>
		<property name="city" value="Noida"></property>
		<property name="state" value="UttarPardesh"></property>
		<property name="country" value="India"></property>
		</bean>        
        
        

		<bean id="employee" class="core.spring.Employee">
		<property name="id" value="121"></property>
		<property name="name" value="RRR"></property>
		<property name="address" ref="address1"></property>
		</bean>
	
		
	
	
	
</beans>
        